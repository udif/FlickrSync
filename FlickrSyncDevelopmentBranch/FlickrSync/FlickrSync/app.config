<?xml version="1.0"?>
<configuration>
	<configSections>
		<sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="FlickrSync.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
		</sectionGroup>
		<sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="FlickrSync.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
		</sectionGroup>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog"/>
	</configSections>
	<userSettings>
		<FlickrSync.Properties.Settings>
   <setting name="FlickrToken" serializeAs="String">
    <value />
   </setting>
   <setting name="LocalInfoXml" serializeAs="Xml">
    <value>
     <FlickrSync />
    </value>
   </setting>
   <setting name="Method" serializeAs="String">
    <value>SyncFilename</value>
   </setting>
   <setting name="NoDelete" serializeAs="String">
    <value>False</value>
   </setting>
   <setting name="NoDeleteTags" serializeAs="String">
    <value>False</value>
   </setting>
   <setting name="MessageId" serializeAs="String">
    <value>090130_0000</value>
   </setting>
   <setting name="UseThumbnailImages" serializeAs="String">
    <value>True</value>
   </setting>
   <setting name="ProxyHost" serializeAs="String">
    <value />
   </setting>
   <setting name="ProxyPort" serializeAs="String">
    <value />
   </setting>
   <setting name="ProxyUser" serializeAs="String">
    <value />
   </setting>
   <setting name="ProxyPass" serializeAs="String">
    <value />
   </setting>
   <setting name="ProxyUse" serializeAs="String">
    <value>False</value>
   </setting>
   <setting name="OrderType" serializeAs="String">
    <value>OrderDefault</value>
   </setting>
   <setting name="AutoSave" serializeAs="String">
    <value>True</value>
   </setting>
   <setting name="LogLevel" serializeAs="String">
    <value>LogAll</value>
   </setting>
   <setting name="MessageLevel" serializeAs="String">
    <value>MessagesAll</value>
   </setting>
  </FlickrSync.Properties.Settings>
	</userSettings>
	<applicationSettings>
		<FlickrSync.Properties.Settings>
   <setting name="FlickrApiKey" serializeAs="String">
    <value>99835d77b01b776f3115b56180c6c4f4</value>
   </setting>
   <setting name="FlickrShared" serializeAs="String">
    <value>3433f92c86ec38f3</value>
   </setting>
   <setting name="Extensions" serializeAs="Xml">
    <value>
     <ArrayOfString xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:xsd="http://www.w3.org/2001/XMLSchema">
      <string>jpg</string>
      <string>jpeg</string>
      <string>gif</string>
      <string>png</string>
      <string>tif</string>
      <string>tiff</string>
      <string>bmp</string>
     </ArrayOfString>
    </value>
   </setting>
   <setting name="HelpMain" serializeAs="String">
    <value>http://flickrsync.freehostia.com/index.htm</value>
   </setting>
   <setting name="HelpProperties" serializeAs="String">
    <value>http://flickrsync.freehostia.com/index_properties.htm</value>
   </setting>
   <setting name="MessageUrl" serializeAs="String">
    <value>http://flickrsync.freehostia.com/message.htm</value>
   </setting>
   <setting name="AdUrl" serializeAs="String">
    <value>http://flickrsync.freehostia.com/ad.htm</value>
   </setting>
   <setting name="DonateUrl" serializeAs="String">
    <value>http://flickrsync.freehostia.com/donate.htm</value>
   </setting>
  </FlickrSync.Properties.Settings>
	</applicationSettings>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets async="true">
      <target name="LogFile" xsi:type="File"
              layout="${longdate} | Type: ${level} | Message: ${message}"
              fileName="${specialfolder:folder=ApplicationData}/FlickrSync/FlickrSyncLog.txt"
              keepFileOpen="false"
              archiveFileName="${specialfolder:folder=ApplicationData}/FlickrSync/LogArchive${longdate}.txt"
              archiveEvery="Day"
              archiveNumbering="Rolling"
              maxArchiveFiles="7" />
      <target name="DebugLogFile" xsi:type="File"
              layout="${longdate} | Method: ${callsite} | Type: ${level} | Message: ${message}"
              fileName="${specialfolder:folder=ApplicationData}/FlickrSync/FlickrSyncDebugLog.txt"
              keepFileOpen="false" deleteOldFileOnStartup="true" />
    </targets>
    <rules>
      <logger name="LogAll" writeTo="LogFile" minlevel="Info" />
      <logger name="LogBasic" writeTo="LogFile" minlevel="Warn" />
      <logger name="LogDebug" writeTo="DebugLogFile" minlevel="Debug" />
    </rules>
  </nlog>
	<startup/></configuration>
